{
  "controller": {
  },
  "workers": [
    {
      "type": "router",
      "realms": [
        {
          "name": "realm1",
          "roles": [
            {
              "name": "anonymous",
              "permissions": [
                {
                  "uri": "*",
                  "publish": true,
                  "subscribe": true,
                  "call": true,
                  "register": true
                }
              ]
            }
          ]
        }
      ],
      "transports": [
        {
          "type": "websocket",
          "endpoint": {
            "type": "tcp",
            "port": 9000
          },
          "auth": {
            "wampcra": {
              "type": "dynamic",
              "authenticator": "com.example.authenticate"
            }
          }
        },
        {
          "type": "web",
          "endpoint": {
            "type": "tcp",
            "port": "$PORT"
          },
          "paths": {
            "/": {
              "type": "static",
              "directory": "../web"
            },
            "ws": {
              "type": "websocket",
              "auth": {
                "wampcra": {
                  "type": "dynamic",
                  "authenticator": "com.example.authenticate"
                }
              }
            }
          }
        }
      ]
    },
    {
      "type": "guest",
      "executable": "node",
      "arguments": [
        "authenticator.js",
        "ws://127.0.0.1/ws",
        "realm1",
        "authenticator1",
        "secret123"
      ],
      "options": {
        "workdir": "../node"
      }
    },
    {
      "type": "guest",
      "executable": "node",
      "arguments": [
        "hello.js",
        "ws://127.0.0.1/ws",
        "realm1",
        "backend1",
        "secret123"
      ],
      "options": {
        "workdir": "../node"
      }
    }
  ]
}
